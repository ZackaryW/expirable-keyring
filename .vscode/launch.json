    "version": "0.2.0",
    "configurations": [
        {
            "name": "run get mysecret",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "secret","get", "MY_TEST"
            ],
            "cwd": "${workspaceFolder}"
