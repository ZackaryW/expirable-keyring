        {
            "name": "run set mysecret",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "secret","set", "MY_TEST", "123456", "in 1 minute"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
